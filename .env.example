# Development Environment (.env untuk local development)
# Copy file ini ke .env dan sesuaikan

APP_NAME="SISKABOE Politala"
APP_ENV=local
APP_KEY=base64:your_app_key_here
APP_DEBUG=true
APP_TIMEZONE=Asia/Jakarta
APP_URL=http://localhost:8000

# Database (SQLite untuk development)
DB_CONNECTION=sqlite
DB_DATABASE=C:\Users\dhani\Desktop\perkuliahan semester 3\IT Project 1\project\siskaobepolitala\database\database.sqlite

# WhatsApp Configuration (Fonnte API)
# Fonnte adalah layanan cloud-based, tidak perlu Node.js!
# Daftar di https://fonnte.com untuk mendapatkan token

# Enable/Disable WhatsApp
WHATSAPP_ENABLED=false

# Fonnte API Token (dapatkan dari dashboard fonnte.com)
FONNTE_TOKEN=your_fonnte_token_here

# Nomor admin untuk menerima notifikasi kontak
WHATSAPP_ADMIN_NUMBER=628xxxxxxxxxx

# Mail (Untuk development, gunakan mailtrap/mailpit)
MAIL_MAILER=log
MAIL_HOST=localhost
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@politala.ac.id"
MAIL_FROM_NAME="${APP_NAME}"
